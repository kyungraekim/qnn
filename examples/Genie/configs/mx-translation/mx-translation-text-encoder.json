{
  "text-encoder" : {
    "version" : 1,
    "type" : "basic",
    "context": {
      "version": 1,
      "n-vocab": 40960,
      "ctx-size": 512,
      "embed-size" : 512,
      "pad-token" : 0
    },
    "tokenizer" : {
      "version" : 1,
      "path" : "your/path/to/tokenizer_file.json"
    },
    "prompt": {
      "version" : 1,
      "prompt-template": ["","</s>"]
    },
    "truncate-input" : true,
    "engine": {
      "version": 1,
      "n-threads" : 6,
      "backend" : {
        "version" : 1,
        "type" : "QnnGenAiTransformer",
        "QnnGenAiTransformer" : {
          "version" : 1
        }
      },
      "model" : {
        "version" : 1,
        "type" : "library",
        "library" : {
          "version" : 1,
          "model-bin" : "your/path/to/model_encoder.bin"
        }
      }
    }
  }
}
