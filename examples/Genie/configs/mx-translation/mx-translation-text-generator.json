{
  "text-generator": {
    "version": 1,
    "type": "basic",
    "accumulator-size" : 1048576,
    "embedding": {
      "version": 1,
      "type": "lut",
      "lut-path": "your/path/to/LUT.bin",
      "size": 512,
      "datatype": "float32"
    },
    "context": {
      "version": 1,
      "size": 512,
      "n-vocab": 40960,
      "bos-token": 0,
      "eos-token": 1
    },
    "tokenizer": {
      "version": 1,
      "path": "your/path/to/tokenizer_file.json"
    },
    "sampler": {
      "version": 1,
      "type": "basic",
      "seed": 100,
      "temp": 1.0,
      "top-k": 1,
      "top-p": 1.0
    },
    "engine": {
      "version" : 1,
      "n-threads" : 6,
      "backend" : {
        "version" : 1,
        "type" : "QnnGenAiTransformer",
        "QnnGenAiTransformer" : {
          "version" : 1,
          "model-input": "embeddings"
        }
      },
      "model" : {
        "version" : 1,
        "type" : "library",
        "library" : {
          "version" : 1,
          "model-bin" : "your/path/to/model_decoder.bin"
        }
      }
    }
  }
}
